<Window x:Class="ADsWarehouse.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADsWarehouse"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Get table" HorizontalAlignment="Left" Height="36" Margin="10,10,0,0" VerticalAlignment="Top" Width="88" Click="Button_Click"/>
        <DataGrid x:Name="dgv" HorizontalAlignment="Center" Height="355" Margin="0,69,0,0" VerticalAlignment="Top" Width="780" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Button Content="Login" HorizontalAlignment="Left" Height="36" Margin="115,10,0,0" VerticalAlignment="Top" Width="88" Click="Button_Click_1"/>
    </Grid>
</Window>
